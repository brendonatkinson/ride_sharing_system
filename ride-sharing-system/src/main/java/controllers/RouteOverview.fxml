<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane prefHeight="300.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.RouteController">
   <children>
      <Pane prefHeight="300.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label layoutX="14.0" layoutY="14.0" text="Routes" />
            <TableView fx:id="routeTable" layoutX="14.0" layoutY="31.0" prefHeight="200.0" prefWidth="558.0">
              <columns>
                <TableColumn fx:id="nameColumn" prefWidth="75.0" text="Name" />
                <TableColumn fx:id="stopColumn" prefWidth="482.0" text="Stops" />
              </columns>
            </TableView>
            <Button layoutX="14.0" layoutY="245.0" mnemonicParsing="false" onAction="#handleNewRoute" text="New Route" />
            <Button layoutX="103.0" layoutY="245.0" mnemonicParsing="false" onAction="#handleEditRoute" text="Edit Route" />
            <Button layoutX="190.0" layoutY="245.0" mnemonicParsing="false" onAction="#handleDeleteRoute" text="Delete Route" />
         </children>
      </Pane>
   </children>
</AnchorPane>
